<Manager_button>:
    id:nav4
    icon: "account-cog"
    ripple_scale: 0
    user_font_size: "30sp"
    theme_text_color: "Custom"
    text_color: get_color_from_hex(Colors().darkgrey)
    pos_hint: {'center_y': .5}
    on_release:
        app.screens.ids.mainscreen.ids.manager.transition.direction = 'left'
        app.screens.ids.mainscreen.ids.manager.current = 'managerscreen'    